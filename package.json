{
  "name": "nfl-flag",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "BABEL_ENV=dev sudo node src/devServer.js",
    "lint": "tslint -c config/tslint.json 'src/**/*.ts*' -e 'src/**/*.d.ts*'",
    "lint:local:jenkins:": "tslint -c config/tslint.json --formatters-dir ./node_modules/tslint-formatters/lib/tslint/formatters --format yaml 'src/**/*.ts*' -e 'src/**/*.d.ts*' | ./node_modules/.bin/tslint-formatters-convert yaml2checkstyle > tslint-results.xml",
    "lint:jenkins": "$WORKSPACE/node_modules/.bin/tslint -c config/tslint.json --formatters-dir ./node_modules/tslint-formatters/lib/tslint/formatters --format yaml 'src/**/*.ts*' -e 'src/**/*.d.ts*' | ./node_modules/.bin/tslint-formatters-convert yaml2checkstyle > tslint-results.xml",
    "clean": "rm -rf ts-build && rm -rf dist",
    "build:dev": "BABEL_ENV=dev webpack --config ./config/webpack.config.dev.js",
    "build:int": "BABEL_ENV=int ENV=int webpack --config ./config/webpack.config.int.js",
    "build:int:jenkins": "BABEL_ENV=int ENV=int $WORKSPACE/node_modules/.bin/webpack --config $WORKSPACE/config/webpack.config.int.js",
    "build:uat": "BABEL_ENV=uat ENV=uat webpack --config ./config/webpack.config.uat.js",
    "build:uat:jenkins": "BABEL_ENV=uat ENV=uat $WORKSPACE/node_modules/.bin/webpack --config $WORKSPACE/config/webpack.config.uat.js",
    "build:prd": "BABEL_ENV=prd ENV=prd webpack --config ./config/webpack.config.prd.js",
    "build:prd:jenkins": "BABEL_ENV=prd ENV=prd $WORKSPACE/node_modules/.bin/webpack --config $WORKSPACE/config/webpack.config.prd.js",
    "open": "open dist/index.html",
    "open:local": "open http://local.nflflag.com",
    "open:index": "open dist/index.html",
    "open:int": "open https://int.nflflag.com",
    "open:uat": "open https://uat.nflflag.com",
    "open:prd": "open https://www.nflflag.com"
  },
  "keywords": [],
  "author": "USA Football",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.13.1",
    "immutable": "^3.8.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-router": "^2.6.1"
  },
  "devDependencies": {
    "@types/axios": "^0.9.28",
    "@types/react": "^0.14.27",
    "@types/react-dom": "^0.14.13",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "3.13.0",
    "tslint-formatters": "0.1.2",
    "typescript": "^2.0.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1",
    "webpack-fail-plugin": "^1.0.5",
    "webpack-notifier": "^1.3.0"
  }
}
